{
  "version": 2,
  "builds": [
    {
      "src": "my-landing-page/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "my-landing-page/dist",
        "buildCommand": "cd my-landing-page && npm install && npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/my-landing-page/dist/$1"
    }
  ]
} 